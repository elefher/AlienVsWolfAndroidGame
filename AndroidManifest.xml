<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elefher.wolfhuntergame"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />

    <application
        android:icon="@drawable/icon"
        android:label="WolfHunter" >
        <activity
            android:name="com.elefher.wolfhuntergame.SampleGame"
            android:configChanges="keyboard|keyboardHidden|orientation"
            android:label="WolfHunter"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.elefher.wolfhuntergame.CreatePlayer"
            android:label="create players">
        </activity>
        <activity
            android:name="com.elefher.wolfhuntergame.NewLevelGame"
            android:label="new level game">
        </activity>
        <activity
            android:name="com.elefher.wolfhuntergame.Licences"
            android:label="Licenses">
        </activity>
        <activity
            android:name="com.elefher.wolfhuntergame.TheEnd"
            android:label="The End">
        </activity>
    </application>

</manifest>